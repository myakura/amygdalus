{
  "name": "amygdalus",
  "version": "1.0.0",
  "scripts": {
    "lint-js": "eslint src/js",
    "lint": "npm run lint-js",
    "watch-js": "onchange 'src/js/**/*.js' -- npm run lint-js",
    "watch": "npm run watch-js",
    "serve": "live-server",
    "start-dev": "concurrently 'npm run serve src/' 'npm run watch' --kill-others",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/myakura/amygdalus.git"
  },
  "author": "Masataka Yakura",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/myakura/amygdalus/issues"
  },
  "homepage": "https://github.com/myakura/amygdalus#readme",
  "devDependencies": {
    "concurrently": "^3.4.0",
    "eslint": "^3.17.0",
    "live-server": "^1.2.0",
    "onchange": "^3.2.1"
  }
}
